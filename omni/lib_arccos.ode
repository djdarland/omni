diff ( y , x , 1 ) =  arcsin (x + 0.0001) ;
!
max_terms := 30;
Digits := 32;
!
x_start := c(-0.999);
x_end := c(0.999) ;
diff(y,0,exact_soln_y(x_start));
glob_look_poles := true;
#
# Not Given = 0
# Real = 1
# Complex = 2 
# No Pole = 3
# Impossible Eq = 4
#
glob_type_given_pole := 0;
!
exact_soln_y := proc(x)
return((c(x) + c(0.0001)) * arcsin(c(x) + c(0.0001)) + sqrt((c(1.0) - (c(x) + c(0.0001)) *  (c(x) +c(0.0001)) )));
end;

