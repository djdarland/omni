diff ( y , x , 1 ) =  sin(0.3 * x + 0.1) - (0.1 * x + 0.2) ;
!
Digits := 32;
max_terms := 40;
!
x_start := c(0.1);
x_end := c(5.0) ;
diff(y,0,exact_soln_y(x_start));
glob_look_poles := true;
#
# Not Given = 0
# Real = 1
# Complex = 2 
# No Pole = 3
# Impossible Eq = 4
#
glob_type_given_pole := 3;

!
exact_soln_y := proc(x)
# return(-0.05 * x * x - 0.2 * x - cos(0.3 * x + 0.1) / 0.3);
return(c(-0.05) * c(x) * c(x) - c(0.2) * c(x) - cos(c(0.3) * c(x) + c(0.1)) / c(0.3));
end;
