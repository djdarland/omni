diff ( y , x , 1 ) =  sin(x) * (0.2 * x + 0.3);
!
Digits := 32;
max_terms := 30;
!
x_start := c(0.1);
x_end := c(5.0) ;
diff(y,0,exact_soln_y(x_start));
glob_look_poles := true;
#
# Not Given = 0
# Real = 1
# Complex = 2 
# No Pole = 3
# Impossible Eq = 4
#
glob_type_given_pole := 3;

!
exact_soln_y := proc(x)
return(c(0.2) * sin(c(x)) - c(0.2) * cos(c(x)) * c(x) - c(0.3) * cos(c(x))); 
end;

