diff ( y , x , 2 ) = diff ( y , x , 1 ) ;
!
Digits := 32;
max_terms := 30;
!
x_start := c(-5.0);
x_end := c(-1.0) ;
diff(y,0,exact_soln_y(x_start));
diff(y,1,exact_soln_yp(x_start));
glob_look_poles := true;
#
# Not Given = 0
# Real = 1
# Complex = 2 
# No Pole = 3
# Impossible Eq = 4
#
glob_type_given_pole := 3;
!
exact_soln_y := proc(x)
return(c(1.0) + exp(c(x)));
end;
exact_soln_yp := proc(x)
return(exp(c(x)));
end;
exact_soln_ypp := proc(x)
return(exp(c(x)));
end;

